<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/parseWithCoroutine"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Parse Entity With Coroutine" />

    <Button
        android:id="@+id/parse"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Parse Entity" />

    <Button
        android:id="@+id/parseListWithCoroutine"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Parse List With Coroutine" />

    <Button
        android:id="@+id/parseList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Parse List" />
</LinearLayout>